0000-03af : PCI Bus 0000:00
  0000-001f : dma1
  0020-0021 : pic1
  0040-0043 : timer0
  0050-0053 : timer1
  0060-0060 : keyboard
  0062-0062 : EC data
  0064-0064 : keyboard
  0066-0066 : EC cmd
  0070-0071 : rtc0
  0080-008f : dma page reg
  00a0-00a1 : pic2
  00c0-00df : dma2
  00f0-00ff : fpu
  0290-029f : pnp 00:02
03b0-03df : PCI Bus 0000:00
03e0-0cf7 : PCI Bus 0000:00
  03f8-03ff : serial
  0400-0453 : pnp 00:09
    0400-0403 : ACPI PM1a_EVT_BLK
    0404-0405 : ACPI PM1a_CNT_BLK
    0408-040b : ACPI PM_TMR
    0410-0415 : ACPI CPU throttle
    0420-042f : ACPI GPE0_BLK
    0450-0450 : ACPI PM2_CNT_BLK
  0454-0457 : pnp 00:0a
  0458-047f : pnp 00:09
  04d0-04d1 : pnp 00:06
  0500-057f : pnp 00:09
0cf8-0cff : PCI conf1
0d00-ffff : PCI Bus 0000:00
  1180-119f : pnp 00:09
  c000-cfff : PCI Bus 0000:09
    c000-c0ff : 0000:09:00.0
  d000-dfff : PCI Bus 0000:08
    d000-d01f : 0000:08:00.0
      d000-d01f : ahci
    d020-d023 : 0000:08:00.0
      d020-d023 : ahci
    d030-d037 : 0000:08:00.0
      d030-d037 : ahci
    d040-d043 : 0000:08:00.0
      d040-d043 : ahci
    d050-d057 : 0000:08:00.0
      d050-d057 : ahci
  e000-efff : PCI Bus 0000:01
    e000-e0ff : 0000:01:00.0
  f000-f01f : 0000:00:1f.3
  f020-f03f : 0000:00:1f.2
    f020-f03f : ahci
  f040-f05f : 0000:00:19.0
  f060-f063 : 0000:00:1f.2
    f060-f063 : ahci
  f070-f077 : 0000:00:1f.2
    f070-f077 : ahci
  f080-f083 : 0000:00:1f.2
    f080-f083 : ahci
  f090-f097 : 0000:00:1f.2
